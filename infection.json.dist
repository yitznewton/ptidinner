{
    "timeout": 10,
    "source": {
        "directories": [
            "src\/DinnerBundle"
        ],
        "excludes": [
            "Controller",
            "Command",
            "Form",
            "Tests"
        ]
    },
    "logs": {
        "text": "infection.log"
    },
    "mutators": {
        "@default": true,
        "GreaterThan": {
          "ignore": [
            "DinnerBundle\\Entity\\Ad::truncatedCopy"
          ]
        },
        "MethodCallRemoval": {
          "ignore": [
            "DinnerBundle\\EventListener\\AdTypeAccession::updateTypeAccession"
          ]
        },
        "PublicVisibility": {
          "ignore": ["*"]
        }
    }
}
