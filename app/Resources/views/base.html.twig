<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}PTI Dinner Accounting{% endblock %}</title>
    {% block stylesheets %}
        {% stylesheets filter="scssphp" output="css/app.css" output="css/dinner.css"
        "bundles/dinner/scss/*.scss" %}
        <link rel="stylesheet" href="{{ asset_url }}"/>
        <link href="https://fonts.googleapis.com/css?family=Lato:400,700|Rubik:400,700&amp;subset=hebrew" rel="stylesheet">
        {% endstylesheets %}
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>
{% block body %}{% endblock %}
{% block javascripts %}
    {% javascripts 'bundles/dinner/js/*.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
</body>
</html>
